<idea-plugin>
    <id>com.cjamcu.getx-snippets</id>
    <name>GetX Snippets</name>
    <vendor email="cjamcu@gmail.com" url="http://www.yourcompany.com">cjamcu</vendor>

    <description>An extension to accelerate the process of developing applications with flutter, aimed at everyone using the GetX package.</description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->

    <depends>com.intellij.java</depends>
    <depends>com.intellij.modules.androidstudio</depends>
    <version>1.0.0</version>



    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplatesProvider implementation="FlutterTemplateProvider" />
        <liveTemplateContext implementation="FlutterContext" />
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>